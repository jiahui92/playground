{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react",
    "esModuleInterop": true,
    "types": ["@types/gtag.js", "@types/chrome"],
    "paths": {
      "@/*": ["./*"],
    },
    "baseUrl": "."
  }
}
